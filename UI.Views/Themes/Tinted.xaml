    <Color x:Key="HeaderForeground">#FFF5F5F5</Color>
    <Color x:Key="MainForeground">#FF708499</Color>
    <Color x:Key="GroupBoxColor">#FF182533</Color>


    <SolidColorBrush x:Key="MenuHeaderBrush" options:Freeze="True" Color="{StaticResource MenuHeader}" />
