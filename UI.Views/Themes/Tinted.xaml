    <SolidColorBrush x:Key="MainForegroundBrush" options:Freeze="True" Color="{StaticResource MainForeground}" />
    <SolidColorBrush x:Key="GroupBoxBackground" options:Freeze="True" Color="{StaticResource GroupBoxColor}" />

